{
  "name": "mossengine/fivecode",
  "type": "library",
  "description": "PHP Class to enable fifth generation instructional code driven by programmatic instructions to execute under a controlled logic in the backend.",
  "keywords": [
    "backend",
    "5GL",
    "programmatic",
    "code",
    "execute",
    "json",
    "array",
    "instruction"
  ],
  "homepage": "https://github.com/Mossengine/FiveCode",
  "support": {
    "issues": "https://github.com/Mossengine/FiveCode/issues",
    "source": "https://github.com/Mossengine/FiveCode"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Brendon Moss",
      "email": "brenmoss@mossengine.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "ramsey/uuid": "3.4.1",
    "illuminate/support": "5.2.32"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~2",
    "phpunit/phpunit": "~4.0 || ~5.0"
  },
  "autoload": {
    "psr-4": {
      "Mossengine\\FiveCode\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": [
      "@phpunit",
      "@phpcs"
    ],
    "phpunit": "phpunit",
    "phpcs": "php-cs-fixer fix -v --diff --dry-run"
  }
}
